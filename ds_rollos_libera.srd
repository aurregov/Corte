table(column=(type=long updatewhereclause=yes name=cs_rollo dbname="cs_rollo" )
 column=(type=long updatewhereclause=yes name=cs_ordenpd_pt dbname="cs_ordenpd_pt" )
 column=(type=decimal(2) updatewhereclause=yes name=ca_mt dbname="ca_mt" )
 column=(type=decimal(2) updatewhereclause=yes name=ca_kg dbname="ca_kg" )
 column=(type=long updatewhereclause=yes name=ca_unidades dbname="ca_unidades" )
 column=(type=long updatewhereclause=yes name=sw_completo dbname="sw_completo" )
 column=(type=decimal(3) updatewhereclause=yes name=kg_liberados dbname="kg_liberados" )
 retrieve="  SELECT temp_tela_n.cs_rollo,   
         temp_tela_n.cs_ordenpd_pt,   
         temp_tela_n.ca_mt,   
         temp_tela_n.ca_kg,   
         temp_tela_n.ca_unidades ,
         temp_tela_n.sw_completo,
         (select sum(kg_liberados) from dt_consumo_rollos where cs_rollo = temp_tela_n.cs_rollo) as kg_liberados
    FROM temp_tela_n  
   WHERE ( temp_tela_n.usuario = :as_usuario ) AND  
         ( temp_tela_n.co_reftel = :an_co_reftel ) AND  
         ( temp_tela_n.co_caract = :an_co_caract ) AND  
         ( temp_tela_n.ancho_agrupa = :an_ancho OR :an_ancho = 0 ) AND  
         ( temp_tela_n.co_color = :an_color ) AND  
         ( temp_tela_n.cs_tanda = :an_tanda ) AND
			( temp_tela_n.co_talla = :an_talla OR :an_talla = 9999)  and
			((temp_tela_n.co_disenno = :an_disenno and :an_disenno <> '0') or
          (:an_disenno = :an_disenno and :an_disenno = '0'))
           

ORDER BY 7 desc,
               temp_tela_n.ca_kg ASC   
" arguments=(("as_usuario", string),("an_co_reftel", number),("an_co_caract", number),("an_diametro", number),("an_ancho", number),("an_color", number),("an_tanda", number),("an_talla", number),("an_disenno", string)) )
