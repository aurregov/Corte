 retrieve="  SELECT DISTINCT dt_escalasxtela.co_fabrica_exp,   
         dt_escalasxtela.cs_liberacion,   
         dt_escalasxtela.nu_orden,   
         dt_escalasxtela.nu_cut,   
         dt_escalasxtela.co_fabrica_pt,   
         dt_escalasxtela.co_linea,   
         dt_escalasxtela.co_referencia,   
         dt_escalasxtela.co_color_pt,   
         dt_escalasxtela.co_tono,   
         dt_escalasxtela.co_modelo,   
         dt_escalasxtela.co_fabrica_tela,   
         dt_escalasxtela.co_reftel,   
         dt_escalasxtela.co_caract,   
         dt_escalasxtela.diametro,   
         dt_escalasxtela.co_color_tela,
			dt_escalasxtela.co_talla,   
         dt_escalasxtela.ca_unidades,   
         dt_escalasxtela.yield,   
         dt_escalasxtela.fe_creacion,   
         dt_escalasxtela.fe_actualizacion,   
         dt_escalasxtela.usuario,   
         dt_escalasxtela.instancia  
    FROM dt_escalasxtela  
   WHERE ( dt_escalasxtela.co_fabrica_exp = :ai_fabexp ) AND  
         ( dt_escalasxtela.cs_liberacion = :al_liberacion ) AND
			( dt_escalasxtela.nu_orden = :as_po ) AND
			( dt_escalasxtela.nu_cut = :as_cut )

