 retrieve="  SELECT dt_pdnxmodulo.simulacion,   
         dt_pdnxmodulo.co_fabrica,   
         dt_pdnxmodulo.co_planta,   
         dt_pdnxmodulo.co_modulo,   
         dt_pdnxmodulo.co_fabrica_exp,   
         dt_pdnxmodulo.pedido,   
         dt_pdnxmodulo.cs_liberacion,   
         dt_pdnxmodulo.po,   
         dt_pdnxmodulo.co_fabrica_pt,   
         dt_pdnxmodulo.co_linea,   
         dt_pdnxmodulo.co_referencia,   
         dt_pdnxmodulo.co_color_pt,   
         dt_pdnxmodulo.tono,   
         dt_pdnxmodulo.cs_estilocolortono,   
         dt_pdnxmodulo.cs_particion,   
         dt_pdnxmodulo.pedido_po,   
         dt_pdnxmodulo.cs_prioridad,   
         dt_pdnxmodulo.ca_programada,   
         dt_pdnxmodulo.ca_producida,   
         dt_pdnxmodulo.ca_pendiente,   
         dt_pdnxmodulo.co_estado_asigna,   
         dt_pdnxmodulo.co_curva,   
         dt_pdnxmodulo.fe_inicio_prog,   
         dt_pdnxmodulo.fe_fin_prog,   
         dt_pdnxmodulo.fe_requerida_desp,   
         dt_pdnxmodulo.ca_minutos_std,   
         dt_pdnxmodulo.co_tipo_asignacion,   
         dt_pdnxmodulo.ca_personasxmod,   
         dt_pdnxmodulo.cod_tur,  
         dt_pdnxmodulo.minutos_jornada,  
         dt_pdnxmodulo.ind_cambio_estilo,   
         dt_pdnxmodulo.ca_unid_base_dia,   
         dt_pdnxmodulo.orige_uni_base_dia,   
         dt_pdnxmodulo.tipo_empate,   
         dt_pdnxmodulo.unidades_empate,   
         dt_pdnxmodulo.metodo_programa,   
         dt_pdnxmodulo.fuente_dato,   
         dt_pdnxmodulo.fe_creacion,   
         dt_pdnxmodulo.fe_actualizacion,   
         dt_pdnxmodulo.usuario,   
         dt_pdnxmodulo.instancia,   
         m_lineas.de_linea,   
         pacasash.h_preref.de_referencia,   
         m_colores.de_color,
			dt_pdnxmodulo.co_estado_merc,  
         (select min(a.fe_inicio_prog)
          from   dt_pdnxmodulo a
			 where  a.co_fabrica_exp=91
			 and    a.co_planta=2
			 and    a.pedido_po        =dt_pdnxmodulo.pedido_po 
			 and    a.po      =dt_pdnxmodulo.po 
			 and    a.co_fabrica_pt   =dt_pdnxmodulo.co_fabrica_pt 
			 and    a.co_linea      =dt_pdnxmodulo.co_linea 
			 and    a.co_referencia =dt_pdnxmodulo.co_referencia 
			 and    a.co_color_pt   =dt_pdnxmodulo.co_color_pt	) as fe_despacho,
			(select unique peddig.gpa
          from   peddig
			 where  peddig.co_fabrica_exp=dt_pdnxmodulo.co_fabrica_exp
			 and    peddig.pedido		  =dt_pdnxmodulo.pedido) as gpa,
         (select unique pedpend_exp.id_mercado
          from   pedpend_exp
          where  pedpend_exp.co_fabrica_exp=dt_pdnxmodulo.co_fabrica_exp
			 and    pedpend_exp.pedido        =dt_pdnxmodulo.pedido_po 
			 and    pedpend_exp.nu_orden      =dt_pdnxmodulo.po 
			 and    pedpend_exp.co_fabrica    =dt_pdnxmodulo.co_fabrica_pt 
			 and    pedpend_exp.co_linea      =dt_pdnxmodulo.co_linea 
			 and    pedpend_exp.co_referencia =dt_pdnxmodulo.co_referencia 
			 and    pedpend_exp.co_color      =dt_pdnxmodulo.co_color_pt) as id_mercado,
			(select h_liberar_escalas.semana_actual
			 from   h_liberar_escalas
			 where  h_liberar_escalas.co_fabrica_exp=dt_pdnxmodulo.co_fabrica_exp
			 and    h_liberar_escalas.pedido        =dt_pdnxmodulo.pedido
	       and    h_liberar_escalas.cs_liberacion =dt_pdnxmodulo.cs_liberacion) as semana_actual,
			(select h_liberar_escalas.letra_actual
			 from   h_liberar_escalas
			 where  h_liberar_escalas.co_fabrica_exp=dt_pdnxmodulo.co_fabrica_exp
			 and    h_liberar_escalas.pedido        =dt_pdnxmodulo.pedido
	       and    h_liberar_escalas.cs_liberacion =dt_pdnxmodulo.cs_liberacion) as letra_actual,
			dt_pdnxmodulo.ca_proyectada,
			dt_pdnxmodulo.ca_actual,
			dt_pdnxmodulo.ca_numerada, ' ' as aprueba,dt_pdnxmodulo.cs_asignacion
    FROM dt_pdnxmodulo,   
         m_lineas,   
         pacasash.h_preref,   
         m_colores  
   WHERE ( dt_pdnxmodulo.co_fabrica_pt = m_lineas.co_fabrica ) and  
         ( dt_pdnxmodulo.co_linea = m_lineas.co_linea ) and  
         ( dt_pdnxmodulo.co_fabrica_pt = pacasash.h_preref.co_fabrica ) and  
         ( dt_pdnxmodulo.co_linea = pacasash.h_preref.co_linea ) and  
         ( dt_pdnxmodulo.co_referencia = pacasash.h_preref.co_referencia ) and  
         ( dt_pdnxmodulo.co_fabrica_pt = m_colores.co_fabrica ) and  
         ( dt_pdnxmodulo.co_linea = m_colores.co_linea ) and  
         ( dt_pdnxmodulo.co_color_pt = m_colores.co_color ) and  
         ( ( dt_pdnxmodulo.simulacion = :simulacion ) AND  
         ( dt_pdnxmodulo.co_fabrica = :co_fabrica_modulo ) AND  
         ( dt_pdnxmodulo.co_planta = :co_planta_modulo ) AND  
         ( dt_pdnxmodulo.co_modulo = :co_modulo ) )    
