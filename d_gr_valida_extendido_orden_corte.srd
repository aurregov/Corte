 retrieve="SELECT  distinct dt_kamban_corte.co_fabrica,
		dt_kamban_corte.co_linea,
		m_lineas.de_linea,
		dt_kamban_corte.co_referencia,
		h_preref_pv.de_referencia,
		(SELECT count(*)  
		 FROM m_instruccionxtipo,	dt_instxestilo_tip , m_tipo_instruccion
		WHERE ( m_instruccionxtipo.co_tipo_instruccio = dt_instxestilo_tip.co_tipo_instruccio ) and  
				( m_instruccionxtipo.co_instruccion = dt_instxestilo_tip.co_instruccion ) and  
				( m_tipo_instruccion.co_tipo_instruccio = dt_instxestilo_tip.co_tipo_instruccio ) and
				 ( dt_instxestilo_tip.co_fabrica = dt_kamban_corte.co_fabrica) AND  
				( dt_instxestilo_tip.co_linea = dt_kamban_corte.co_linea ) AND  
				( dt_instxestilo_tip.co_referencia = dt_kamban_corte.co_referencia) and
				( m_instruccionxtipo.co_tipo_instruccio = 3) and
				(  m_instruccionxtipo.de_instruccion <> '' )) extendido,
	(Select max(co_estado_asigna) 
	From dt_pdnxmodulo
	Where cs_asignacion = dt_kamban_corte.cs_orden_corte) estado_orden,
    (Select count(*) From dt_observa_oc
     Where cs_orden_corte = :al_orden_corte) tiene_obs

 FROM dt_kamban_corte,
		h_preref_pv,
		m_lineas     
WHERE ( dt_kamban_corte.co_fabrica 			= h_preref_pv.co_fabrica ) and 
		( dt_kamban_corte.co_linea 			= h_preref_pv.co_linea ) and 
		( dt_kamban_corte.co_referencia 		= h_preref_pv.co_referencia ) and 
      ( h_preref_pv.co_tipo_ref           = 'P') and
		( dt_kamban_corte.co_fabrica 			= m_lineas.co_fabrica ) and 
		( dt_kamban_corte.co_linea 			= m_lineas.co_linea ) and 
		( ( dt_kamban_corte.cs_orden_corte 	= :al_orden_corte) )" arguments=(("al_orden_corte", number)) )
