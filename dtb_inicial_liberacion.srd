 column=(type=long updatewhereclause=yes name=co_tipo_orden dbname="co_tipo_orden" values="Individual	1/Agrupada	2/" )
 column=(type=long updatewhereclause=yes name=co_fabrica_planta dbname="co_fabrica_planta" )
 column=(type=long updatewhereclause=yes name=co_planta dbname="co_planta" )
 column=(type=char(60) updatewhereclause=yes name=de_tipo_agrupa dbname="de_tipo_agrupa" )
 column=(type=long updatewhereclause=yes name=op_agrupada dbname="op_agrupada" )
