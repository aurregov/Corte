 column=(type=long updatewhereclause=yes name=co_cliente dbname="co_cliente" )
 column=(type=char(70) updatewhereclause=yes name=de_cliente dbname="de_cliente" )
 column=(type=char(30) updatewhereclause=yes name=gpa dbname="gpa" )
 column=(type=long updatewhereclause=yes name=rgb dbname="rgb" )
 column=(type=number updatewhereclause=yes name=porc_cumplimiento dbname="porc_cumplimiento" )
 column=(type=char(1) updatewhereclause=yes name=tiempo dbname="tiempo" )
 column=(type=long updatewhereclause=yes name=co_linea_exp dbname="co_linea_exp" )
 column=(type=long updatewhereclause=yes name=prioridad dbname="prioridad" )
 column=(type=long updatewhereclause=yes name=falta_tlla dbname="falta_tlla" )
