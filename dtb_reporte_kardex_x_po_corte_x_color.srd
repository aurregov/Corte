 retrieve="select DISTINCT tp.usuario usuario,
       tp.co_fabrica fabrica,
       tp.co_linea as linea, 
       tp.de_linea,
       tp.co_referencia as cod_referencia, 
       tp.de_referencia, 
       tp.co_cliente, 
       tp.co_fabrica_exp, 
       tp.po as nu_po,
       tp.nu_cut as nu_cut,
       tp.fe_despacho as fe_despacho,
       tp.co_color as color, 
       tp.co_color_exp as color_exp,
       tp.ca_pedida as unid_pedido,
       tp.ca_enviada as unid_enviadas, 
       tp.ca_pendiente,
       tc.cs_orden_corte as orden_corte,
       tc.fe_creacion_oc,
       (nvl(tc.prog_oc,0)) as prog_oc, 
       (nvl(tc.cortado_oc,0)) as cortado_oc,
       (nvl(tc.x_despachar_oc,0)) as por_despachar, 
       SUM(nvl(tc.unidades_x,0)),
       (select sum(ca_pedida) from temp_rep_po_prog
        where usuario = tp.usuario
          AND co_fabrica = tp.co_fabrica
          AND co_linea = tp.co_linea)  as tot_ped_linea,
      (select sum(ca_enviada) from temp_rep_po_prog
        where usuario = tp.usuario
          AND co_fabrica = tp.co_fabrica
          AND co_linea = tp.co_linea)  as tot_env_linea,
       ( tp.ca_enviada / tp.ca_pedida) as porcent_enviado

     
from temp_rep_po_prog tp,  outer temp_rep_po_corte tc
where tp.usuario = tc.usuario
  and tp.co_fabrica = tc.co_fabrica 
  and tp.co_linea = tc.co_linea
  and tp.co_referencia = tc.co_referencia
  and tp.co_cliente = tc.co_cliente
  and tp.co_fabrica_exp = tc.co_fabrica_exp
  and tp.po = tc.po
  and tp.nu_cut = tc.nu_cut
  and tp.co_color = tc.co_color
  and tp.co_color_exp = tc.co_color_exp
  and tp.ca_pedida > 0
 
  and tp.usuario = :as_usuario
