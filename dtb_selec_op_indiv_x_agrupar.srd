 retrieve="select unique agrupacion_pedido.cs_ordenpd_pt, agrupacion_pedido.co_Ref_exp,agrupacion_pedido.co_referencia, agrupacion_pedido.co_talla,
           (select count(*) from dt_caxpedidos where cs_ordenpd_pt = agrupacion_pedido.cs_ordenpd_pt
             and co_color = agrupacion_pedido.co_color) as tot_tllas
from agrupacion_pedido, h_ordenpd_pt
where agrupacion_pedido.cs_ordenpd_pt_agrupa = :al_ordenpd_pt_agrupa
and agrupacion_pedido.cs_agrupa_lib = :ai_cs_agrupa_lib
and agrupacion_pedido.co_color = :ai_color
and agrupacion_pedido.tipo_agrupacion > 1
and agrupacion_pedido.cs_ordenpd_pt = h_ordenpd_pt.cs_ordenpd_pt
and h_ordenpd_pt.co_estado_orden not in (5,6)
and (select sum(ca_programada) from dt_caxpedidos d
        where d.cs_ordenpd_pt = agrupacion_pedido.cs_ordenpd_pt
            and d.co_color = agrupacion_pedido.co_color
            and d.co_talla = agrupacion_pedido.co_talla) > 0


