 column=(type=decimal(0) updatewhereclause=yes name=unid_real_liberar dbname="unid_real_liberar" )
 column=(type=decimal(4) updatewhereclause=yes name=kilos dbname="kilos" )
 retrieve="  SELECT co_reftel,   
         co_caract,   
         diametro,   
         ancho,   
         co_color_te,   
         tono, 
			co_modelo,
			co_fabrica,
			co_linea,
			co_referencia,
			co_color,  
			cs_tanda,
			nu_cut,
         co_estampado,
         (select co_ttejido from h_telas where co_reftel = temp_modelos_ref.co_Reftel and co_caract = 0) as co_ttejido,
         sum(unid_real_liberar) unid_real_liberar,
			sum(unid_real_liberar * consumo) kilos
   
    FROM temp_modelos_ref   
   WHERE sw_marca 		<> 3 AND
			usuario 			= :as_usuario AND
			cs_ordenpd_pt 	= :an_ordenpd_pt AND
			po 				= :as_po AND
			co_color 		= :an_color AND
			sw_carga			= 1
GROUP BY co_reftel,   
         co_caract,   
         diametro,   
         ancho,   
         co_color_te,   
         tono,
			co_modelo,
			co_fabrica,
			co_linea,
			co_referencia,
			co_color,
		   cs_tanda,
			nu_cut,
         co_estampado,
         15
ORDER BY 17 asc
" arguments=(("as_usuario", string),("an_ordenpd_pt", number),("as_po", string),("an_color", number),("an_ancho", number),("an_tanda", number))  sort="co_modelo D " )
