table(column=(type=long updatewhereclause=yes name=cs_orden_corte dbname="cs_orden_corte" )
 column=(type=long updatewhereclause=yes name=raya dbname="raya" )
 column=(type=char(60) updatewhereclause=yes name=de_modelo dbname="de_modelo" )
 column=(type=char(20) updatewhereclause=yes name=po dbname="po" )
 column=(type=char(30) updatewhereclause=yes name=gpa dbname="gpa" )
 column=(type=long updatewhereclause=yes name=tipo dbname="tipo" )
 column=(type=char(32) updatewhereclause=yes name=usuario dbname="usuario" )
 column=(type=decimal(0) updatewhereclause=yes name=agrup_pedidos dbname="agrup_pedidos" )
 retrieve="  SELECT dt_trazosxoc.cs_orden_corte,   
         h_base_trazos.raya,
		pr_descripcion_raya(dt_trazosxoc.cs_orden_corte, h_base_trazos.raya) de_modelo,
			(select max(po) 
				from dt_pdnxmodulo
				where cs_asignacion = dt_trazosxoc.cs_orden_corte ) as po,
		(  SELECT MAX( peddig.gpa )
			 FROM dt_pdnxmodulo,   
					dt_pedidosxorden,   
					peddig  
			WHERE ( dt_pdnxmodulo.cs_ordenpd_pt = dt_pedidosxorden.cs_ordenpd_pt ) and  
					( dt_pedidosxorden.pedido = peddig.pedido ) and  
					( dt_pedidosxorden.nu_orden = peddig.orden_compra ) and  
					( dt_pdnxmodulo.cs_asignacion = dt_trazosxoc.cs_orden_corte )
		) gpa,
         (select h.co_tipo
          from h_ordenes_corte h
          where h.cs_orden_corte = dt_trazosxoc.cs_orden_corte) tipo,
		user usuario,

		(select count(a.cs_asignacion)
		from dt_pdnxmodulo a, dt_op_agrupa_lib_talla b
		where a.cs_liberacion = b.cs_liberacion
		and a.cs_asignacion =  dt_trazosxoc.cs_orden_corte) as agrup_pedidos

    FROM h_base_trazos,   
         dt_trazosxoc
   WHERE ( dt_trazosxoc.cs_agrupacion 				= h_base_trazos.cs_agrupacion ) and  
         ( dt_trazosxoc.cs_base_trazos 			= h_base_trazos.cs_base_trazos ) and  
         ( dt_trazosxoc.cs_orden_corte 			= :an_ordencorte ) And
         ( ( :an_sw_sesgo = 0 And pr_descripcion_raya(dt_trazosxoc.cs_orden_corte, h_base_trazos.raya) NOT LIKE '%SESGO%' ) Or
          ( :an_sw_sesgo = 1 And pr_descripcion_raya(dt_trazosxoc.cs_orden_corte, h_base_trazos.raya) LIKE '%SESGO%' ) Or
		(  :an_sw_sesgo = 2 ) )

Group by 1, 2" arguments=(("an_ordencorte", number),("an_sw_sesgo", number))  sort="raya A " )
