 retrieve="  SELECT admtelas.h_mercada.cs_mercada,   
         admtelas.h_mercada.cs_reposicion,   
         admtelas.h_mercada.co_tipo_mercada,   
         admtelas.h_mercada.co_estado_mercada,   
         admtelas.h_mercada.cs_orden_corte,   
         admtelas.h_mercada.co_centro_corte,   
         ( select Count(distinct dt_rollosmercada.cs_rollo) 
			from admtelas.dt_rollosmercada
			where  ( admtelas.dt_rollosmercada.cs_mercada = admtelas.h_mercada.cs_mercada ) ) total_rollos,
			fe_actualizacion,
			usuario
    FROM admtelas.h_mercada  
   WHERE admtelas.h_mercada.cs_mercada = :as_mercada 
" update="admtelas.h_mercada" updatewhere=1 updatekeyinplace=yes arguments=(("as_mercada", number)) )
