         pedpend_exp.grupo_empaque,   
         pedpend_exp.single_item,   
         pedpend_exp.ca_bolsa_x_caja,   
         pedpend_exp.peso,   
         pedpend_exp.volumen,   
         pedpend_exp.tejido,   
