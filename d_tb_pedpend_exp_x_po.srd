         pedpend_exp.opc_ped14,   
         pedpend_exp.opc_ped15,			
			pedpend_exp.ompro,
         pedpend_exp.ca_comprada,
			pedpend_exp.ca_liberada,
         pedpend_exp.ca_pedida_comprar,
         pedpend_exp.ca_inicial,
         pedpend_exp.co_prepack,
			( Case When pedpend_exp.co_calidad = 1 then
				'S'
			Else 
				'N' 
			End ) seleccion,
			pedpend_exp.cantidad_102,
			/*(NVL(( SELECT de_referencia_crta
					FROM h_preref 
					WHERE h_preref.co_fabrica = pedpend_exp.co_fabrica AND
						 h_preref.co_linea = pedpend_exp.co_linea AND
						 h_preref.co_referencia = pedpend_exp.co_referencia  
				), '-0-')
			)*/ '' de_referencia,
			pedpend_exp.omdiv,
			pedpend_exp.cusol,
			pedpend_exp.cuship,
			pedpend_exp.categoria_sap,
			pedpend_exp.co_material_sap,
			pedpend_exp.co_talla_sap,
			pedpend_exp.co_color_sap,
			( select max(p.item) 
				from pedpend_exp p
				where p.pedido = peddig.pedido) item_primera,
			pedpend_exp.ca_tejido,
			peddig.tipo_pedido,
			pedpend_exp.tipo_orden_toc,
			pedpend_exp.toc
    FROM pedpend_exp, peddig  
   WHERE pedpend_exp.pedido = peddig.pedido
	and ( pedpend_exp.nu_orden = :an_nu_orden ) 
