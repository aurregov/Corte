 retrieve="SELECT	h_ordenpd_pt.cs_ordenpd_pt ,
			h_ordenpd_pt.co_fabrica ,
			h_ordenpd_pt.co_linea ,
			m_lineas.de_linea ,
			h_ordenpd_pt.co_referencia ,
			h_preref_pv.de_referencia ,
			dt_caxpedidos.co_color ,
			m_color.de_color ,
			m_tallas_x_grupo_pv.de_talla ,
			dt_caxpedidos.ca_programada ,
			dt_caxpedidos.co_talla,
			dt_caxpedidos.ca_liberadas,
         m_tallas_x_grupo_pv.orden_talla ,
             dt_pdnxmodulo.ca_unid_base_dia 
FROM	h_preref_pv ,
		dt_caxpedidos ,
		h_ordenpd_pt ,
		m_color ,
		m_lineas ,
		m_tallas_x_grupo_pv,
         dt_pdnxmodulo    
WHERE ( h_ordenpd_pt.cs_ordenpd_pt = dt_caxpedidos.cs_ordenpd_pt ) and          
		( h_ordenpd_pt.co_fabrica = h_preref_pv.co_fabrica ) and          
		( h_ordenpd_pt.co_linea = h_preref_pv.co_linea ) and          
      ( Cast(h_ordenpd_pt.co_referencia as char(15) ) = h_preref_pv.co_referencia ) and
      ( trim(h_preref_pv.co_tipo_ref) = 'P' ) and      
	   ( dt_caxpedidos.co_color = m_color.co_color ) and          
		( h_ordenpd_pt.co_fabrica = m_lineas.co_fabrica ) and          
		( h_ordenpd_pt.co_linea = m_lineas.co_linea ) and          
		( h_preref_pv.co_grupo_tlla = m_tallas_x_grupo_pv.co_grupo_tlla ) and     
      CAST(dt_caxpedidos.co_talla as CHAR(10)) = m_tallas_x_grupo_pv.co_talla   and
		( ( h_ordenpd_pt.cs_ordenpd_pt = :al_ordenpdn ) and     
         (  dt_pdnxmodulo.cs_liberacion = :al_liberacion ) and
         (  dt_pdnxmodulo.tipo_empate <> 5 ) and
          ( dt_pdnxmodulo.cs_ordenpd_pt = h_ordenpd_pt.cs_ordenpd_pt ) and  
		( dt_caxpedidos.co_color = :ai_color ) ) 

UNION
SELECT	h_ordenpd_pt.cs_ordenpd_pt ,
			h_ordenpd_pt.co_fabrica ,
			h_ordenpd_pt.co_linea ,
			m_lineas.de_linea ,
			h_ordenpd_pt.co_referencia ,
			h_preref_pv.de_referencia ,
			dt_caxpedidos.co_color ,
			m_color.de_color ,
			m_tallas_x_grupo_pv.de_talla ,
			dt_caxpedidos.ca_programada ,
			dt_caxpedidos.co_talla,
			dt_caxpedidos.ca_liberadas,
             m_tallas_x_grupo_pv.orden_talla,
             dt_pdnxmodulo.ca_unid_base_dia   
FROM	h_preref_pv ,
		dt_caxpedidos ,
		h_ordenpd_pt ,
		m_color ,
		m_lineas ,
		m_tallas_x_grupo_pv,
         dt_pdnxmodulo,
         dt_op_agrupa_lib_talla    
WHERE (dt_op_agrupa_lib_talla.cs_ordenpd_pt = h_ordenpd_pt.cs_ordenpd_pt ) and
           (dt_op_agrupa_lib_talla.cs_liberacion = dt_pdnxmodulo.cs_liberacion ) and
          ( dt_op_agrupa_lib_talla.co_talla  =  dt_caxpedidos.co_talla ) and
          ( dt_op_agrupa_lib_talla.co_color  =  dt_caxpedidos.co_color ) and
         ( h_ordenpd_pt.cs_ordenpd_pt = dt_caxpedidos.cs_ordenpd_pt ) and          
		( h_ordenpd_pt.co_fabrica = h_preref_pv.co_fabrica ) and          
		( h_ordenpd_pt.co_linea = h_preref_pv.co_linea ) and          
      ( Cast(h_ordenpd_pt.co_referencia as char(15) ) = h_preref_pv.co_referencia ) and
      ( trim(h_preref_pv.co_tipo_ref) = 'P' ) and      
	   ( dt_caxpedidos.co_color = m_color.co_color ) and          
		( h_ordenpd_pt.co_fabrica = m_lineas.co_fabrica ) and          
		( h_ordenpd_pt.co_linea = m_lineas.co_linea ) and          
		( h_preref_pv.co_grupo_tlla = m_tallas_x_grupo_pv.co_grupo_tlla ) and     
      CAST(dt_caxpedidos.co_talla as CHAR(10)) = m_tallas_x_grupo_pv.co_talla   and
		( (  dt_pdnxmodulo.cs_liberacion = :al_liberacion ) and
         (  dt_pdnxmodulo.tipo_empate = 5 ) and
  		( dt_caxpedidos.co_color = :ai_color ) ) 

ORDER BY 1,13,11 ASC;" arguments=(("al_ordenpdn", number),("ai_color", number),("al_liberacion", number))  sort="h_ordenpd_pt_cs_ordenpd_pt A m_color_de_color A co_talla A " )
