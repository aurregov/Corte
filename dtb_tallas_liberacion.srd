 column=(type=long updatewhereclause=yes name=dt_orden_tllaclor_co_color dbname="co_color" )
 column=(type=char(30) updatewhereclause=yes name=m_color_de_color dbname="de_color" )
 column=(type=char(20) updatewhereclause=yes name=m_tallas_x_grupo_de_talla dbname="de_talla" )
